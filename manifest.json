{
    "manifest_version": 2,
    "name": "ChatGPT Extension",
    "version": "1.0",
    "description": "An extension that interacts with the ChatGPT API",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "images/icon48.png",
      }
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  